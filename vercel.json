{
  "builds": [
    {
      "src": "frontend/dist/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist"
    }
  ]
}
